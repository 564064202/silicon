CREATE TABLE msc_sessions (
   key TEXT NOT NULL,
   user_id INT);

CREATE TABLE msc_users (
   id INTEGER PRIMARY KEY NOT NULL,
   email           TEXT    NOT NULL,
   password        BLOB     NOT NULL
   );


CREATE TABLE msc_songs (
   id INTEGER PRIMARY KEY NOT NULL,
   user_id INT NOT NULL,
   title           TEXT    NOT NULL,
   filename        TEXT     NOT NULL);
